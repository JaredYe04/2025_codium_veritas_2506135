document.addEventListener("alpine:init",(()=>{Alpine.data("listCollapse",(()=>({open:!1,othersOpen:!1,buttonElement:null,init(){this.buttonElement=this.$el;const e=this.buttonElement.querySelector("button").innerText;this.buttonElement.setAttribute("aria-label",`open ${e} menu`),window.addEventListener("list-collapse-close",(()=>{this.othersOpen=!1})),window.addEventListener("list-collapse-open",(()=>{this.open=!1,this.othersOpen=!0}))},handleKeyDown(e){switch(e.key){case"ArrowDown":case"Enter":case"Space":e.preventDefault(),this.toggleMenu();break;default:return null}},toggleMenu(){this.open?this.$dispatch("list-collapse-close"):(this.$dispatch("list-collapse-open"),this.othersOpen=!0),this.open=!this.open}})))}));