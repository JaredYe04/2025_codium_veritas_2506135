let popup_datalayer=(o,e="open",a="N/A")=>{let t={event:`popup_${e}`,formName:"N/A",formType:"N/A",buttonLocation:"N/A"};if(o&&nfForms){for(let e of nfForms)e.id===o&&(formName=e.settings.title);formName&&(t.formName=formName,t.formType="ninja form",t.buttonLocation=a)}window.dataLayer=window.dataLayer||[],window.dataLayer.push(t)};const nlsn_unescape_quotes=o=>o.includes("&quot;")?DOMPurify.sanitize(o.replaceAll("&quot;",'"')):o;